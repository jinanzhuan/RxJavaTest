<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <Button
        android:onClick="zip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="zip"/>

    <Button
        android:onClick="merge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="merge"/>

    <Button
        android:onClick="start_with"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start_with"/>

    <Button
        android:onClick="combine_latest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="combine_latest"/>

    <Button
        android:onClick="jion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="jion"/>

    <Button
        android:onClick="switch_on_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="switch_on_next"/>

