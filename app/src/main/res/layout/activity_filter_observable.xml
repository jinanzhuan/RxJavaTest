<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:onClick="debounce"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="debounce"/>

        <Button
            android:onClick="distinct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="distinct"/>

        <Button
            android:onClick="element_at"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ElementAt"/>

        <Button
            android:onClick="filter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="filter"/>

        <Button
            android:onClick="first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="first"/>

        <Button
            android:onClick="ignore_element"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ignore_element"/>

        <Button
            android:onClick="last"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="last"/>

        <Button
            android:onClick="sample"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="sample"/>

        <Button
            android:onClick="skip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="skip"/>

        <Button
            android:onClick="skip_last"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="skip_last"/>

        <Button
            android:onClick="take"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="take"/>

        <Button
            android:onClick="take_last"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="take_last"/>

    </LinearLayout>



</ScrollView>